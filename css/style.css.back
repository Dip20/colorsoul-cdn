:root {
  --font-family: "Montserrat-Light";
}

html {
  scroll-behavior: smooth;
}

/* scrollbar */
body::-webkit-scrollbar-track {
  width: 1px;
  background: #341c11;
}

body::-webkit-scrollbar {
  background: linear-gradient(20deg, #341c11, #341c11) border-box;
  width: 1px;
}

body::-webkit-scrollbar-thumb {
  border-radius: 30px;
  width: 1px;
  height: 1px;
  background: linear-gradient(20deg, #341c11, #341c11) border-box;
}

body {
  overflow-y: scroll;
}

